{
  "name": "nextjs-notion-starter-kit",
  "version": "2.0.0",
  "private": true,
  "description": "The perfect starter kit for building beautiful websites with Next.js and Notion.",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "repository": "transitive-bullshit/nextjs-notion-starter-kit",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "deploy": "vercel deploy",
    "deps": "run-s deps:*",
    "deps:update": "[ -z $GITHUB_ACTIONS ] && yarn add notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:update on CI'",
    "deps:link": "[ -z $GITHUB_ACTIONS ] && yarn link notion-client notion-types notion-utils react-notion-x || echo 'Skipping deps:link on CI'",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "test": "run-p test:*",
    "test:lint": "eslint '**/*.{ts,tsx}'",
    "test:prettier": "prettier '**/*.{js,jsx,ts,tsx}' --check",
    "check-node-compatibility": "check-node-version --node 20"
  },
  "dependencies": {
    "@fisch0920/use-dark-mode": "^2.4.0",
    "@giscus/react": "^2.3.0",
    "@keyvhq/core": "^1.6.9",
    "@keyvhq/redis": "^1.6.10",
    "@types/lodash": "^4.17.4",
    "@types/lodash.omit": "^4.5.9",
    "@vercel/analytics": "^1.3.1",
    "@vercel/og": "^0.0.19",
    "batch-processor": "^1.0.0",
    "check-node-version": "^4.2.1",
    "classnames": "^2.3.1",
    "commander": "^9.5.0",
    "date-fns": "^2.28.0",
    "element-resize-detector": "^1.2.4",
    "expiry-map": "^2.0.0",
    "fathom-client": "^3.4.1",
    "got": "^12.0.3",
    "isomorphic-unfetch": "^4.0.2",
    "lodash.omit": "^4.5.0",
    "lqip-modern": "^2.0.0",
    "next": "^12",
    "notion-client": "^6.16.0",
    "notion-types": "^6.16.0",
    "notion-utils": "^6.16.0",
    "p-map": "^5.3.0",
    "p-memoize": "^6.0.1",
    "performance-now": "^2.1.0",
    "posthog-js": "^1.20.2",
    "raf": "^3.4.1",
    "random": "^4.1.0",
    "react": "^18.2.0",
    "react-body-classname": "^1.3.1",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-notion-x": "^6.16.0",
    "react-sizeme": "^3.0.2",
    "react-tweet-embed": "^2.0.0",
    "react-use": "^17.4.2",
    "rss": "^1.2.2",
    "seedrandom": "^3.0.5",
    "shallowequal": "^1.1.0",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "^18.8.5",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "cross-env": "^7.0.2",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "overrides": {
    "cacheable-request": {
      "keyv": "npm:@keyvhq/core@~1.6.6"
    }
  }
}
